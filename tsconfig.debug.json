{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "outDir": "./lib",
    "sourceMap": true,

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
}



