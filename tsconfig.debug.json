{
  "compilerOptions": {
    "target": "ES2016",
    "module": "ESNext",
    "outDir": "./lib",
    "sourceMap": true,
    "paths": {
      "bootstrap":["./node_modules/bootstrap/dist/js/bootstrap.js"]
    },

    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true
  },
}



