{
	"name": "@printf83/bsts",
	"version": "0.1.62",
	"description": "Bootstrap TS builder",
	"keywords": [
		"bootstrap 5",
		"api",
		"sdk",
		"typescript"
	],
	"author": "Hamzah A <printf83@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/printf83/bsts/issues"
	},
	"homepage": "https://github.com/printf83/bsts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/printf83/bsts.git"
	},
	"main": "./lib/index.mjs",
	"types": "./lib/types/index.d.ts",
	"exports": {
		"types": "./lib/types/index.d.ts",
		"default": "./lib/index.mjs"
	},
	"files": [
		"lib/**/*"
	],
	"scripts": {
		"clean": "rm -rf ./lib",
		"clean:win": "rmdir /s lib",
		"build": "npm run clean && npm run build:esm && mv ./lib/index.js ./lib/index.mjs",
		"build:win": "npm run clean:win && npm run build:esm && mv ./lib/index.js ./lib/index.mjs",
		"build:esm": "tsc -p ./configs/tsconfig.esm.json",
		"debug": "tsc -p ./configs/tsconfig.esm.json --watch",
		"prepack": "npm run build"
	},
	"devDependencies": {
		"@types/bootstrap": "^5.2.6",
		"typescript": "^5.0.3"
	}
}
