{
	"name": "@printf83/bsts",
	"version": "0.1.8",
	"description": "Bootstrap TS builder",
	"keywords": [
		"bootstrap 5",
		"api",
		"sdk",
		"typescript"
	],
	"author": "Hamzah A <printf83@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/printf83/bsts/issues"
	},
	"homepage": "https://github.com/printf83/bsts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/printf83/bsts.git"
	},
	"main": "lib/index.js",
	"exports": {
		"require": "./lib/index.js",
		"default": "./lib/index.js"
	},
	"scripts": {
		"clear": "tsc --build --clean tsconfig.src.json && tsc --build --clean tsconfig.doc.json && clear",
		"build": "tsc --project tsconfig.src.json && tsc --project tsconfig.doc.json",
		"src": "tsc --project tsconfig.src.json --watch",
		"doc": "tsc --project tsconfig.doc.json --watch"
	},
	"devDependencies": {
		"@types/bootstrap": "^5.2.6",
		"@types/js-beautify": "^1.13.3",
		"typescript": "^5.0.1-rc"
	}
}
