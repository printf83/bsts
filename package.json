{
	"name": "@printf83/bsts",
	"version": "0.1.0",
	"description": "Bootstrap TS builder",
	"main": "dist/index.js",
	"module": "dist/index.m.js",
	"unpkg": "dist/index.umd.js",
	"types": "dist/index.d.ts",
	"exports": {
		"require": "./dist/index.js",
		"default": "./dist/index.modern.js"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "rm -rf dist && microbundle --tsconfig tsconfig.json --no-sourcemap",
		"dev": "microbundle --watch --tsconfig tsconfig.json --no-sourcemap"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/printf83/bsts.git"
	},
	"keywords": [
		"bootstrap 5",
		"api",
		"sdk",
		"typescript"
	],
	"author": "Hamzah A <printf83@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/printf83/bsts/issues"
	},
	"homepage": "https://github.com/printf83/bsts#readme",
	"devDependencies": {
		"typescript": "^4.9.3"
	}
}
