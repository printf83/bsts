{
	"name": "@printf83/bsts",
	"version": "0.2.1",
	"description": "Bootstrap TS builder",
	"keywords": [
		"bootstrap 5",
		"typescript"
	],
	"author": "Hamzah A <printf83@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/printf83/bsts/issues"
	},
	"homepage": "https://github.com/printf83/bsts#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/printf83/bsts.git"
	},
	"main": "./build/esm/index.js",
	"types": "./build/types/index.d.ts",
	"exports": {
		".": {
			"types": "./build/types/index.d.ts",
			"require": "./build/cjs/index.js",
			"import": "./build/esm/index.js",
			"default": "./build/esm/index.js"
		}
	},
	"devDependencies": {
		"@types/bootstrap": "^5.2.6",
		"@types/node": "^20.4.1",
		"typescript": "^5.1.6"
	},
	"dependencies": {
		"bootstrap": "^5.3.0"
	},
	"scripts": {
		"link": "npm link",
		"compile": "tsc -b ./tsconfig.cjs.json ./tsconfig.esm.json ./tsconfig.types.json",
		"build:clean": "rm -rf ./build",
		"build:clean:win": "rmdir /s /q lib",
		"prepare": "node ./scripts/prepare-package-json",
		"build": "npm run build:clean && npm run compile",
		"build:win": "npm run build:clean:win && npm run compile",
		"build:prepare": "npm run build:clean && npm run compile && npm run prepare",
		"build:win:prepare": "npm run build:clean:win && npm run compile && npm run prepare",
		"prepack": "npm run build",
		"prepack:win": "npm run build:win"
	}
}
